/* Page/Club/CreateComplete/createcomplete.wxss */
@import "iconfont_individual_details.wxss";
.wrap{width: 100%;height: auto;overflow: hidden}
.wrap .content{width: 100%;height: 100%;background: #fff;}
.name{display: block;width: 90%;line-height: 80rpx;font-size: 32rpx;color: #000;margin: 0 auto 20rpx auto;}
.wrap .content .content_line{width: 90%;line-height: 90rpx;margin: 0 auto;border-bottom: 1px solid rgb(214, 211, 211);}
.wrap .content .content_line .line_descript{font-size: 36rpx;color: #000;float: left;}
.wrap .content .content_line .line_val{font-size: 32rpx;color: #999;float: right}
.introduction{width:72%;overflow:hidden;text-overflow: ellipsis;white-space: nowrap;text-align: right}
.wrap .content .content_line icon{float: right}
.line{display: flex;width: 90%;margin: 0 auto;}
.line button{flex: 1;margin-bottom: 20rpx;border: none;background: #2196f3;color: #fff}


