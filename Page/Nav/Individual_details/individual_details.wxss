.content .content_btns{
  width: auto;
  height: 34px;
  line-height: 33px;
  display: block;
  text-align: center;
  
}
.content .content_btns button{
  border: none;
  background-color: #2196f3;
  color: #fff;
  font-size: 13px;
}
.content .content_btns button::after{
  border: 1px solid #2196f3;
}
.content .content_btns .look_bfen{
  width: 44%;
