/* Page/Nav/Individual_details/individual_details.wxss */
@import "iconfont_individual_details.wxss";
.body{
  width: 100%;
  height: 100%;
}
.head{
  width: 95%;
  height: auto;
  line-height: 25px;
  margin: 0 auto;
  font-size: 16px;
  padding: 2px 4px;
  background-color:rgb(248, 248, 248);
}
.content{
  width: 100%;
  height: auto;
}
.content .content_wrap{
  width: 96%;
  height: 100%;
  margin: 0 auto 15px auto;
  background-color: #fff;
}
.content .content_wrap .content_line{
  width: auto;
  line-height: 39px;
  font-size: 13px;
  padding: 0 8px;
  border-bottom: 1px solid #eee;
}
.content .content_wrap .content_line icon{
  display: inline-block;
  margin-right: 5px;
  color: #2196f3;
}
