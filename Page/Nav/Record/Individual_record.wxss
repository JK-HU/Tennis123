/* Page/Nav/Record/Individual_record.wxss */
@import "iconfont_individual_details.wxss";
.body{
  width: 100%;
  height: 100%
}
.header{
  width: 100%;
  height: auto;
  display: block;
}
.header .header_title{
  width: 100%;
  line-height: 20px;
  text-align: center;
  display: block;
  margin: 20px auto 0 auto;
}
.header .jiang{
  width: 100%;
  line-height: 30px;
  display: block;
  text-align: center;
  margin: 18px auto;
}
.header .jiang .icona{font-size: 1.4rem;color: rgb(255, 174, 0);}
.header .jiang .iconb{font-size: 1.4rem;color: rgb(138, 135, 135);}
.header .jiang text{font-size: 1.4rem;color: rgb(88, 161, 243);margin-right: 9px;}
.header .js_jl{
  width: 120px;
  height: 35px;
  font-size: 14px;
  border: none;
  background: #2196f3;
  color: #fff;
}

.content{
  width: 100%;
  height: auto;
  margin-top: 27px;
  background: #fff;
}
.content .content_wrap{
  width: 95%;
  height: auto;
  margin: 0 auto;
}
.content .content_wrap .bs_type{
  width: 100%;
  display: block;
  line-height: 40px;
  border-top: 1px solid #eee;
  border-bottom: 1px solid #eee;
  font-size: 15px;
  color: #999;
}
.content .content_wrap .line_views {
  width: 100%;
  line-height: 40px;
  border-bottom: 1px solid #eee;
  font-size: 13px;
  color: #999
}
.content .content_wrap .line_views .status{
  padding: 3px 5px;
  background-color: rgb(192, 11, 11);
  color: #fff;
  font-size: 12px;
  border-radius: 4px;
  line-height: 10px;
}
.content .content_wrap .line_views .d_s{font-size: 15px;color: #000;margin-right: 5px;}
.content .content_wrap .line_views .description{}
.content .content_wrap .line_views icon{float: right;}

