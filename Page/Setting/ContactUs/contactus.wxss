/* Page/Setting/ContactUs/contactus.wxss */
.wrap{width: 100%;height: 100%}
.content{width: 70%;height: auto;margin: 0 auto}
.content .logo{width: 100%;height: auto}
.content .logo image{width: 120rpx;height: 120rpx;display: block;margin: 16rpx auto;}
.content .logo text{font-size: 24rpx;color: #999;display: block;margin-bottom: 40rpx;text-align: center}
.content .phone{width: 100%;height: auto}
.content .phone text{font-size: 26rpx;display: block;width: 100%;line-height: 45rpx;text-align: center;}
.content .ewm{width: 100%;height: auto;}
.content .ewm text{font-size: 30rpx;width: 100%;display: block;text-align: center;margin-top: 60rpx;}
.content .ewm image{display: block;width: 400rpx;height: 400rpx;background: #fff;margin: 16rpx auto}



