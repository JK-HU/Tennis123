/* Page/Setting/ModifyMes/modifymes.wxss */
@import "iconfont_individual_details.wxss";
.wrap{width: 100%;height: 100%}
.content{width: 95%;height: auto;margin: 0 auto;}
.content .line{width: auto;line-height: 60rpx;font-size: 28rpx;padding: 8rpx 16rpx;border-bottom: 1px solid #ddd;position: relative}
.content .line text{float: left}
.content .line input{display: inline-block;width: 75%;line-height: 60rpx;margin-left:40rpx;float:left;}
.content .line icon{position: absolute;right: 16rpx;top: 50%;transform: translateY(-50%)}
.content .line .picker{width: 63%;text-align: center;float: right}

